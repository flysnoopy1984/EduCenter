@page
@model EduCenterWeb.Pages.WebBackend.User.ListModel
@{
    ViewData["Title"] = "List";
    Layout = "~/Pages/Shared/_EduMainPCWeb.cshtml";
}
@section navBar{
    <li><a href="/WebBackend/Home">主页</a></li>
}
<h2>用户列表</h2>
<link href="~/lib/layui/css/layui.css" rel="stylesheet" />
<script src="~/lib/layui/layui.js"></script>
<script src="~/lib/layui/lay/modules/laypage.js"></script>

<script src="~/js/WebBackEnd/User/List.js"></script>
<style type="text/css">
    .OptionArea {
        margin: 10px;
    }

    #UserListTable {
        width: 100%;
        font-size: 11px;
    }

        #UserListTable tr th {
            border: solid 1px #808080;
            text-align:center;
        }

        #UserListTable tr td {
            border: solid 1px #808080;
        }
</style>

<div class="MainContainer">
    <div class="OptionArea">
        <input id="btnQuery" type="button" class="btn btn-primary btn-sm" value="查询" />
    </div>
    <div class="DataArea">
        <table id="UserListTable" border="0" cellpadding="0" cellspacing="0">
            <tr>
                <th width="20%">操作</th>
                <th>用户微信名</th>
                <th>宝贝姓名</th>
                <th>会员类型</th>
                <th>标准班剩余课时</th>
                <th>标准班有效期</th>
                <th>暑假班剩余课时</th>
                <th>暑假班有效期</th>
                <th>寒假班剩余课时</th>
                <th>寒假班有效期</th>
                <th>允许选择标准课</th>
                <th>允许选择假期课</th>
            </tr>

        </table>

        <div class="Pager"></div>
    </div>


</div>
